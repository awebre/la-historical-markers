{
  "cli": {
    "version": ">= 3.8.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "BUILD_NUMBER": "39",
        "VERSION": "1.5",
        "API_URL": "https://api.lahistoricalmarkers.com",
        "PHOTOS_URL": "https://photos.lahistoricalmarkers.com/marker-photos",
        "GITHUB_URL": "https://github.com/awebre/la-historical-markers"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
